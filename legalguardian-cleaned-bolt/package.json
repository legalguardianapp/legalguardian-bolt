{
  "name": "legal-guardian-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "start": "expo start",
    "build:web": "expo export --platform web",
    "build": "expo export --platform web",
    "deploy": "expo export --platform web && netlify deploy --prod --dir dist",
    "lint": "expo lint",
    "web": "expo start --web",
    "build:production": "expo export --platform web --output-dir dist",
    "clean": "rm -rf .expo node_modules/.cache dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "expo": "^53.0.0",
    "expo-av": "^15.1.6",
    "expo-camera": "~16.1.5",
    "expo-clipboard": "^7.1.4",
    "expo-constants": "~17.1.3",
    "expo-font": "~13.2.2",
    "expo-linking": "~7.1.3",
    "expo-location": "~18.1.2",
    "expo-print": "~12.8.1",
    "expo-router": "~5.0.2",
    "expo-sharing": "^13.1.5",
    "expo-speech": "^13.1.7",
    "expo-speech-recognition": "~1.1.0",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-system-ui": "~5.0.5",
    "expo-web-browser": "~14.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.1",
    "react-native-reanimated": "~3.8.0",
    "react-native-safe-area-context": "5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0"
